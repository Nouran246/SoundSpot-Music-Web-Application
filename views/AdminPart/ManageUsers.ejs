<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" type="text/css" href="/Admin/Admin%20CSS/Sidenavbar.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <title>SoundSpot</title>
    <link rel="icon" href="/photo/2.png" type="image/png">

    <script src="/Admin/Admin%20JavaScript/Sidenavbar.js"></script>
    <link rel="stylesheet" href="/Admin/Admin%20CSS/ManageUsers.css">
    <script src="/Admin/Admin%20JavaScript/ManageUsers.js"></script>
</head>

<body>
    <h1>User List</h1>
    <% if (users.length > 0) { %>
      
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>phone</th>
                <th>Email</th>
                <th>gender</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.username %></td>
                    <td><%= user.phone %></td>
                    <td><%= user.email %></td>
                    <td><%= user.gender %></td>
                </tr>
                <% }) %>
        </tbody>
        <% } else { %>

            <h2>No users to display</h2>

            <% } %>
    </table>
</body>
</html>
